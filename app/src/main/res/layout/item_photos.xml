<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="5dp"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
        android:id="@+id/imageViewPhotos"
        android:layout_width="61dp"
        android:layout_height="60dp"
        android:src="@drawable/ic_killer"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/itemTextViewPhotos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/imageViewPhotos"
        app:layout_constraintEnd_toEndOf="@id/imageViewPhotos"
        app:layout_constraintTop_toBottomOf="@id/imageViewPhotos"
        android:text="Photo"
        android:textSize="15sp"
        android:textColor="@color/black"
        />


</androidx.constraintlayout.widget.ConstraintLayout>